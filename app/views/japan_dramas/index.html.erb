<!-- <%= link_to 'New Event', new_japan_drama_path %> -->
<table>
<% @dramas.each do |drama| %>
  <tr>
    <td><%= drama.name %></td>
    <td><%= link_to 'Show', japan_drama_path(drama) %></td>
    <td><%= link_to 'Edit', edit_japan_drama_path(drama) %></td>
    <td><%= button_to 'Delete', japan_drama_path(drama), :method => :delete %></td>
  </tr>
<% end %>
</table>

<br>

<span><%= paginate @dramas %></span>

<br><br>

<div>
  <%= form_for @drama do |f| %>
    <%= render 'form', :f => f %>
    <%= f.submit "Create" %>
  <% end %>
</div>